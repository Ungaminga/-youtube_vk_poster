# Переименуй меня в conf.ini и удали строки с комментариями

[YouTube]
ApiKey = # Апи ключ отсюда: https://console.developers.google.com/apis/api/youtube.googleapis.com следуя этому мануалу: https://developers.google.com/youtube/v3/getting-started
